    <div class="page" title="Profile presentation">
      <div class="finalimage" hidden>
        <img src="img/sm.jpeg">
      </div>
      <div class="puzzle">
        <div class="piece p4" onclick="move(this)"></div>
        <div class="piece p3" onclick="move(this)"></div>
        <div class="piece p8" onclick="move(this)"></div>
        <div class="piece p2" onclick="move(this)"></div>
        <div class="piece p5" onclick="move(this)"></div>
        <div class="piece p7" onclick="move(this)"></div>
        <div class="piece p1" onclick="move(this)"></div>
        <div class="piece p6" onclick="move(this)"></div>
        <div class="piece p9" onclick="move(this)"></div>
      </div>

      <script>
        var a = [
          [4, 3, 8],
          [2, 5, 7],
          [1, 6, 9]
        ];
        var done = false;

        function isvalid(x, y) {
          return x >= 0 && x < 3 && y >= 0 && y < 3;
        }

        function check() {
          var res = true;
          for (var i = 0; i < 3; i++) {
            for (var j = 0; j < 3; j++) {
              if (a[i][j] != i * 3 + j + 1) {
                res = false;
                break
              }
            }
          }
          if (res) {
            done = true;
            var puzzle = document.getElementsByClassName('puzzle')[0];
            puzzle.style.display = "none";
            var piece = document.getElementsByClassName('piece');
            for (var i = 0; i < piece.length; i++) {
              piece[i].hidden = true;
            }
            var finalimage = document.getElementsByClassName('finalimage')[0];
            finalimage.hidden = false;
          }
        }

        function move(pie) {
          var cl = pie.classList[1];
          var p = cl[1];
          if (p == 9 || done) {
            return
          }
          var x = 0;
          var y = 0;
          var d = [
            [1, 0],
            [-1, 0],
            [0, 1],
            [0, -1]
          ];
          for (var i = 0; i < 3; i++) {
            for (var j = 0; j < 3; j++) {
              if (a[i][j] == p) {
                x = i;
                y = j;
                break;
              }
            }
          }
          for (var i = 0; i < d.length; i++) {
            var x1 = d[i][0];
            var y1 = d[i][1];
            if (isvalid(x1 + x, y1 + y)) {
              if (a[x1 + x][y1 + y] == 9) {
                a[x1 + x][y1 + y] = parseInt(p);
                a[x][y] = 9;
                var ninth = document.getElementsByClassName('p9')[0]
                pie.classList.toggle("p9");
                pie.classList.toggle(cl);
                ninth.classList.toggle("p9");
                ninth.classList.toggle(cl);
                break;
              }
            }
          }
          check()
        }
      </script>
